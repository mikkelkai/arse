{
  "name": "arse",
  "version": "0.0.1",
  "description": "A shitty library for managing and rendering state",
  "main": "/lib/index.js",
  "repository": {
    "url": "https://github.com/mikkelkai/arse.git",
    "type": "git"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "npm test -- -w",
    "init": "mkdir lib",
    "clean": "rm -rf lib",
    "prebuild": "npm run clean && npm run init",
    "build": "babel src -d lib",
    "pretest": "npm run build",
    "test": "tape -r babel-register ./lib/**/*.test.js | node_modules/.bin/tap-spec"
  },
  "author": "mikkelkai <mikkelkai@gmail.com>",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3"
  }
}
